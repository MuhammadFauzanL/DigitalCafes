<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lat.pam.digitalcafe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DigitalCafe">

        <activity
            android:name=".OrderCompleteActivity"
            android:label="Pesanan Selesai"
            android:exported="false" />
        <activity
            android:name=".OrderSummaryActivity"
            android:label="Ringkasan Pesanan"
            android:exported="false" />

        <activity
            android:name=".WelcomeActivity"
            android:label="@string/app_name"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".AuthActivity"
            android:label="@string/app_name"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:label="Login"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:label="Register"
            android:exported="false" />

        <activity
            android:name=".HomeActivity"
            android:exported="false"
            android:label="DigitalCafe Menu" />
        <activity
            android:name=".DeliveryActivity"
            android:exported="false"
            android:label="Form Pengiriman" />

    </application>

</manifest>